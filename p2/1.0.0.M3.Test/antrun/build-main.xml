<?xml version="1.0" encoding="UTF-8" ?>
<project name="maven-antrun-" default="main"  >
<target name="main">
  <taskdef resource="net/sf/antcontrib/antcontrib.properties"/>
  <copy file="dev-compositeArtifacts.xml" tofile="target/compositeArtifacts.xml"/>
  <copy file="dev-compositeContent.xml" tofile="target/compositeContent.xml"/>
  <copy file="p2.index" todir="target/"/>
  <tstamp>
    <format property="repo.timestamp" pattern="yyMMddHHmmss"/>
  </tstamp>
  <replace summary="yes" file="target/compositeArtifacts.xml" value="${repo.timestamp}" token="%TIMESTAMP%"/>
  <replace summary="yes" file="target/compositeArtifacts.xml" value="org.eclipse.cft-${unqualifiedVersion}.${buildQualifier}" token="%BUILD-ID%"/>
  <replace summary="yes" file="target/compositeContent.xml" value="${repo.timestamp}" token="%TIMESTAMP%"/>
  <replace summary="yes" file="target/compositeContent.xml" value="org.eclipse.cft-${unqualifiedVersion}.${buildQualifier}" token="%BUILD-ID%"/>
</target>
</project>